<?xml version="1.0" encoding="utf-8"?>
<layout
    android:id="@+id/alert_layout_map2"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    <data>
        <import type="android.view.View"/>
    </data>
    <LinearLayout
            android:background="@drawable/mapping_alert_layout_bg"
            android:orientation="vertical"
            android:layout_width="500dp"
            android:layout_height="200dp">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical"
            android:gravity="center_horizontal"
            >
            <View
                android:id="@+id/ellipse_6"
                android:layout_width="67.14dp"
                android:layout_height="67.14dp"
                android:layout_marginTop="16dp"
                android:background="@drawable/mapping_success_icon"
               />
            <TextView
                android:id="@+id/mapping_suc"
                android:layout_width="wrap_content"
                android:layout_height="15dp"
                android:layout_marginTop="10dp"
                android:text="@string/mapping_suc"
                android:textAppearance="@style/mapping_suc"
                android:textColor="@color/black"
                android:gravity="center_horizontal|top"
                />

            <TextView
                android:id="@+id/lgd_code_00"
                android:layout_width="wrap_content"
                android:layout_height="29dp"
                android:textSize="16sp"
                android:layout_marginTop="10dp"
                android:textColor="#23CD7B"
                android:text="@string/lgd_code_00"
                android:textAppearance="@style/lgd_code_00"
                android:gravity="center_horizontal|top"
                />


        </LinearLayout>
    </LinearLayout>
</layout>